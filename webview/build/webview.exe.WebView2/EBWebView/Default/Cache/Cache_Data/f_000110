import * as THREE from "/node_modules/.vite/deps/three.js?v=06c640ef";
// Function to create a block
function createBlock(x, y, z, type, wireframe = false, faces) {
    const geometry = new THREE.BufferGeometry();
    const materials = [];
    /**
     * Left Face
     */
    if (faces['left']) {
        const vertices = new Float32Array([
            x, y, z,
            x, y + 1, z,
            x, y + 1, z + 1,
            x, y, z + 1
        ]);
        const indices = new Uint16Array([
            0, 1, 2,
            0, 2, 3
        ]);
        geometry.setAttribute('position', new THREE.BufferAttribute(vertices, 3));
        geometry.setIndex(new THREE.BufferAttribute(indices, 1));
        materials.push(new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe }));
    }
    /**
     * Right Face
     */
    if (faces['right']) {
        const vertices = new Float32Array([
            x + 1, y, z,
            x + 1, y + 1, z,
            x + 1, y + 1, z + 1,
            x + 1, y, z + 1
        ]);
        const indices = new Uint16Array([
            0, 1, 2,
            0, 2, 3
        ]);
        geometry.setAttribute('position', new THREE.BufferAttribute(vertices, 3));
        geometry.setIndex(new THREE.BufferAttribute(indices, 1));
        materials.push(new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe }));
    }
    /**
     * Top Face
     */
    const block = new THREE.Mesh(geometry, materials);
    return block;
}
export default function createChunk(x, y, z, data, CHUNK_SIZE, CHUNK_HEIGHT, wireframe = false) {
    const chunk = new THREE.Group();
    for (let i = 0; i < CHUNK_SIZE; i++) {
        for (let j = 0; j < CHUNK_HEIGHT; j++) {
            for (let k = 0; k < CHUNK_SIZE; k++) {
                if (data[i][j][k] !== null) {
                    const faces = {
                        left: i === 0 || data[i - 1][j][k] === null,
                        right: i === CHUNK_SIZE - 1 || data[i + 1][j][k] === null,
                        bottom: j === 0 || data[i][j - 1][k] === null,
                        top: j === CHUNK_HEIGHT - 1 || data[i][j + 1][k] === null,
                        back: k === 0 || data[i][j][k - 1] === null,
                        front: k === CHUNK_SIZE - 1 || data[i][j][k + 1] === null
                    };
                    const block = createBlock(x + i, y + j, z + k, data[i][j][k], wireframe, faces);
                    chunk.add(block);
                }
            }
        }
    }
    return chunk;
}

