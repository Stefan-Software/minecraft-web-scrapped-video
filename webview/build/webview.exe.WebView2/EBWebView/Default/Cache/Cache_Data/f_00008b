/**
 * Three.JS
 */
import { PointerLockControls } from "/node_modules/.vite/deps/three_examples_jsm_controls_PointerLockControls.js?v=2bb0c71b";
import * as THREE from "/node_modules/.vite/deps/three.js?v=2bb0c71b";
/**
 * Custom Functions
 */
import createChunk from "/js/createChunk.js?t=1704289557698";
/**
 * Create a scene, camera and renderer
 */
const scene = new THREE.Scene();
export default scene;
const camera = new THREE.PerspectiveCamera(75, window.innerWidth / window.innerHeight, 0.1, 1000);
camera.position.z = 5;
const renderer = new THREE.WebGLRenderer();
renderer.setSize(window.innerWidth, window.innerHeight);
document.body.appendChild(renderer.domElement);
/**
 * Create a chunk
 */
createChunk(3, 'grass', true);
/**
 * Resize the renderer when the window is resized
 */
window.addEventListener('resize', () => {
    // Unlock the pointer
    controls.unlock();
    // Update the camera and renderer
    camera.aspect = window.innerWidth / window.innerHeight;
    camera.updateProjectionMatrix();
    renderer.setSize(window.innerWidth, window.innerHeight);
    // Re-lock the pointer after a delay
    setTimeout(() => {
        controls.lock();
    }, 100);
    window.dispatchEvent(new Event('resize'));
    // Reset the cursor
    document.body.style.cursor = 'default';
});
/**
 * Create the camera mouse control
 */
const controls = new PointerLockControls(camera, document.body);
document.addEventListener('click', function () {
    controls.lock();
}, false);
// Add an event listener to unlock the pointer when the Escape key is pressed
document.addEventListener('keydown', function (event) {
    if (event.key === 'Escape') {
        controls.unlock();
        document.body.style.cursor = 'auto'; // Show the cursor
    }
}, false);
// Movement speed, direction
const speed = 0.1;
const direction = new THREE.Vector3();
/**
 * Handle keyboard input
 */
document.addEventListener('keydown', (event) => {
    switch (event.key) {
        case 'w':
            direction.z = -1;
            break;
        case 's':
            direction.z = 1;
            break;
        case 'a':
            direction.x = -1;
            break;
        case 'd':
            direction.x = 1;
            break;
    }
});
document.addEventListener('keyup', (event) => {
    switch (event.key) {
        case 'w':
        case 's':
            direction.z = 0;
            break;
        case 'a':
        case 'd':
            direction.x = 0;
            break;
    }
});
// Animation loop
function animate() {
    requestAnimationFrame(animate);
    // Update the camera's position
    camera.updateProjectionMatrix();
    if (controls.isLocked === true) {
        camera.translateX(speed * direction.x);
        camera.translateZ(speed * direction.z);
    }
    renderer.render(scene, camera);
}
animate();
/**
 * Menu Controls
 */
let isMenuOpen = false;
const menu = document.querySelector('.menu');
menu.addEventListener('click', function (event) {
    event.stopPropagation();
});
document.addEventListener('keydown', (event) => {
    if (event.key === 'Tab') {
        if (isMenuOpen) {
            menu.style.display = 'none';
            isMenuOpen = false;
            controls.lock();
        }
        else {
            menu.style.display = 'block';
            isMenuOpen = true;
            controls.unlock();
        }
    }
});
document.addEventListener('click', () => {
    if (isMenuOpen) {
        menu.style.display = 'none';
        isMenuOpen = false;
        controls.lock();
    }
});

