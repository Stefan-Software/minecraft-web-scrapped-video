import * as THREE from "/node_modules/.vite/deps/three.js?v=06c640ef";
// Function to create a block
function createBlock(x, y, z, type, wireframe = false, faces) {
    const geometry = new THREE.BufferGeometry();
    // Define the vertices for a cube
    const vertices = [
        // Front face
        -0.5, -0.5, 0.5,
        0.5, -0.5, 0.5,
        0.5, 0.5, 0.5,
        -0.5, 0.5, 0.5,
        // Back face
        -0.5, -0.5, -0.5,
        -0.5, 0.5, -0.5,
        0.5, 0.5, -0.5,
        0.5, -0.5, -0.5,
        // Top face
        -0.5, 0.5, -0.5,
        -0.5, 0.5, 0.5,
        0.5, 0.5, 0.5,
        0.5, 0.5, -0.5,
        // Bottom face
        -0.5, -0.5, -0.5,
        0.5, -0.5, -0.5,
        0.5, -0.5, 0.5,
        -0.5, -0.5, 0.5,
        // Right face
        0.5, -0.5, -0.5,
        0.5, 0.5, -0.5,
        0.5, 0.5, 0.5,
        0.5, -0.5, 0.5,
        // Left face
        -0.5, -0.5, -0.5,
        -0.5, -0.5, 0.5,
        -0.5, 0.5, 0.5,
        -0.5, 0.5, -0.5,
    ];
    // Define the indices for each face
    const indices = [
        0, 1, 2, 0, 2, 3,
        4, 5, 6, 4, 6, 7,
        8, 9, 10, 8, 10, 11,
        12, 13, 14, 12, 14, 15,
        16, 17, 18, 16, 18, 19,
        20, 21, 22, 20, 22, 23, // left
    ];
    // Remove the vertices and indices for the faces that should not be visible
    ['front', 'back', 'top', 'bottom', 'right', 'left'].forEach((face, i) => {
        if (!faces[face]) {
            vertices.splice(i * 12, 12);
            indices.splice(i * 6, 6);
        }
    });
    geometry.setAttribute('position', new THREE.Float32BufferAttribute(vertices, 3));
    geometry.setIndex(indices);
    const materials = [
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe }),
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe }),
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-top.png`), wireframe }),
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-bottom.png`), wireframe }),
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe }),
        new THREE.MeshBasicMaterial({ map: new THREE.TextureLoader().load(`data/textures/${type}/${type}-side.png`), wireframe })
    ];
    const block = new THREE.Mesh(geometry, materials);
    block.position.set(x, y, z);
    return block;
}
export default function createChunk() {
    const chunk = new THREE.Group();
    // Create a 3x3x3 chunk of grass blocks
    for (let x = 0; x < 3; x++) {
        for (let y = 0; y < 1; y++) {
            for (let z = 0; z < 3; z++) {
                chunk.add(createBlock(x, y, z, 'grass', false, {
                    front: true,
                    back: true,
                    top: true,
                    bottom: true,
                    right: true,
                    left: true
                }));
            }
        }
    }
    return chunk;
}

